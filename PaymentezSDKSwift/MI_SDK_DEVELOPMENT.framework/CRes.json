{
  "type": "object",
  "properties": {
    "messageType": {
      "type": "string",
      "pattern": "^$|CRes"
    },
    "messageVersion": {
      "type": "string",
      "pattern": "^$|^\\d.\\d.\\d{1,4}$"
    },
    "sdkTransID": {
      "type": "string",
      "pattern": "^$|^[a-fA-F0-9]{8}-[a-fA-F0-9]{4}-[1-5][a-fA-F0-9]{3}-[89aAbB][a-fA-F0-9]{3}-[a-fA-F0-9]{12}$"
    },
    "threeDSServerTransID": {
      "type": "string",
      "pattern": "^$|^[a-fA-F0-9]{8}-[a-fA-F0-9]{4}-[1-5][a-fA-F0-9]{3}-[89aAbB][a-fA-F0-9]{3}-[a-fA-F0-9]{12}$"
    },
    "acsUiType": {
      "type": "string",
      "pattern": "^$|^0[1-5]$"
    },
    "acsTransID": {
      "type": "string",
      "pattern": "^$|^[a-fA-F0-9]{8}-[a-fA-F0-9]{4}-[1-5][a-fA-F0-9]{3}-[89aAbB][a-fA-F0-9]{3}-[a-fA-F0-9]{12}$"
    },
    "acsCounterAtoS": {
      "type": "string",
      "pattern": "^$|^\\d{3}$"
    },
    "challengeCompletionInd": {
      "type": "string",
      "pattern": "^$|^[YN]$"
    },
    "acsHTML": {
      "type": "string",
      "pattern": "^$|^([A-Za-z0-9_-]{4})*([A-Za-z0-9_-]{4}|[A-Za-z0-9_-]{3}=|[A-Za-z0-9_-]{2}==|[A-Za-z0-9_-]{2,3})$",
      "maxLength": 100000
    },
    "acsHTMLRefresh": {
      "type": "string",
      "pattern": "^$|^([A-Za-z0-9_-]{4})*([A-Za-z0-9_-]{4}|[A-Za-z0-9_-]{3}=|[A-Za-z0-9_-]{2}==|[A-Za-z0-9_-]{2,3})$",
      "maxLength": 100000
    },
    "challengeAddInfo": {
      "type": "string",
      "maxLength": 256
    },
    "challengeInfoHeader": {
      "type": "string",
      "maxLength": 45
    },
    "challengeInfoLabel": {
      "type": "string",
      "maxLength": 45
    },
    "challengeInfoText": {
      "type": "string",
      "maxLength": 350
    },
    "challengeInfoTextIndicator": {
      "type": "string",
      "pattern": "^$|^[YN]$"
    },
    "challengeSelectInfo": {
      "type": "array",
      "items": {
        "type": "object",
        "patternProperties": {
          "^.{0,45}$": { "type": "string", "maxLength": 45 }
        },
        "additionalProperties": false
      }
    },
    "expandInfoLabel": {
      "type": "string",
      "maxLength": 45
    },
    "expandInfoText": {
      "type": "string",
      "maxLength": 256
    },
    "messageExtension": { "type": "array",
      "items": {"type":"object",
      "properties": {
        "name": {
          "type": "string",
          "maxLength": 64
        },
        "id": {
          "type": "string",
          "maxLength": 64
        },
        "criticalityIndicator": {
          "type": "boolean"
        },
        "data": {
          "type": "object",
          "maxSize": 8059
        }},
      "required": ["name", "id", "criticalityIndicator", "data"]},
      "maxItems": 10
    },
    "oobAppURL": {
      "type": "string",
      "maxLength": 256,
      "format": "url"
    },
    "oobAppLabel": {
      "type": "string",
      "maxLength": 45
    },
    "oobContinueLabel": {
      "type": "string",
      "maxLength": 45
    },
    "resendInformationLabel": {
      "type": "string",
      "maxLength": 45
    },
    "submitAuthenticationLabel": {
      "type": "string",
      "maxLength": 45
    },
    "transStatus": {
      "type": "string",
      "pattern": "^$|^[YN]$"
    },
    "whyInfoLabel": {
      "type": "string",
      "maxLength": 45
    },
    "whyInfoText": {
      "type": "string",
      "maxLength": 256
    },
    "issuerImage": {
      "type":"object",
      "properties": {
        "medium": {
          "type": "string",
          "maxLength": 2048,
          "format": "url"
        },
        "high": {
          "type": "string",
          "maxLength": 2048,
          "format": "url"
        },
        "extraHigh": {
          "type": "string",
          "maxLength": 2048,
          "format": "url"
        }
      }
    },
    "psImage": {
      "type":"object",
      "properties": {
        "medium": {
          "type": "string",
          "maxLength": 2048,
          "format": "url"
        },
        "high": {
          "type": "string",
          "maxLength": 2048,
          "format": "url"
        },
        "extraHigh": {
          "type": "string",
          "maxLength": 2048,
          "format": "url"
        }
      }
    }
  }
}
